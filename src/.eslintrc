{  
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },    
    "rules": {
      "func-names": 0,
      "no-plusplus": 0,
      "indent": ["error", 4],
      "no-buffer-constructor": 0,
      "consistent-return": 0,
      "no-nested-ternary": 0,
      "no-use-before-define": 0,
      "no-bitwise": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "import/no-extraneous-dependencies": 0,
      "global-require": 0,
      "radix": 0,
      "new-cap": 0,
      "prefer-template": 0,
      "max-len": 0
    }
}